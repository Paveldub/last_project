include ./common/_mixins
block variables

doctype html
html
  head
    meta(charset="utf-8")
    title #{title}
    meta(content="" name="author")
    meta(content="" name="description")
    meta(content="" name="keywords")
    meta(content="width=device-width, initial-scale=1" name="viewport")
    meta(content="ie=edge" http-equiv="x-ua-compatible")

    //- favicon block start
    link(rel='icon', href='./assets/img/favicon.ico', type='image/x-icon')
    //- favicon block end
    link(rel="stylesheet" href="./assets/css/foundation%=suffix=%.css%=version=%")
    link(rel="stylesheet" href="./assets/css/app%=suffix=%.css%=version=%")
    link(rel="stylesheet", href="https://cdnjs.cloudflare.com/ajax/libs/prism/1.9.0/themes/prism.min.css")
    link(rel="stylesheet", href="https://cdnjs.cloudflare.com/ajax/libs/prism/1.9.0/plugins/line-numbers/prism-line-numbers.min.css")
    link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.min.css')

    script(src="https://cdnjs.cloudflare.com/ajax/libs/prism/1.9.0/prism.min.js")
    script(src="https://cdnjs.cloudflare.com/ajax/libs/prism/1.9.0/plugins/line-numbers/prism-line-numbers.min.js")
    script(src="./assets/js/foundation%=suffix=%.js%=version=%" defer)
    script(src="./assets/js/app%=suffix=%.js%=version=%" defer)
    
    <!--[if lt IE 9]>
    script(src="http://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.2/html5shiv.min.js")
    script(src="http://cdnjs.cloudflare.com/ajax/libs/respond.js/1.4.2/respond.min.js")
    <![endif]-->

